    "# Get the information for the cantons interesting us\n",
    "df_cylindre_canton_GR_SZ = df_cylindre_canton_toplot\\\n",
    "    [(df_cylindre_canton_toplot.index=='GR') | (df_cylindre_canton_toplot.index=='SZ')]\n",
    "\n",
    "# Plot the \"cylinder score\"\n",
    "plot_dataframe(x=df_cylindre_canton_GR_SZ.columns, y=df_cylindre_canton_GR_SZ,\\\n",
