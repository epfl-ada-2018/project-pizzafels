    "\n",
    "# Plot the results\n",
    "plot_dataframe(x=df_day, title=\"Number of accidents per weekday\",\\\n",
    "               xlabel='Number of accidents', ylabel='Weekday', bars='barh',\\\n",
    "               name_save='accident_weekday',margin=70)"
   ]
