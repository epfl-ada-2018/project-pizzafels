   "source": [
    "# Get the information on the gear box for each canton and keep the interesting columns\n",
    "df_boite = df_Voitures[df_Voitures.boite_index != 9].groupby(['canton','boite']).sum()\\\n",
    "        [[d for d in df_Voitures.columns if IS_DATE.match(d)]].reset_index().\\\n",
    "        drop(columns = ['boite'])\n",
    "\n",
    "# Calculate and store the yearly percentage of automatic vehicles by canton\n",
    "df_boite_canton = pd.DataFrame()\n",
    "for cant in dict_cantons.keys():\n",
    "    for year in df_boite.columns:\n",
    "        if year != 'canton':\n",
    "            # Get the data for the corresponding canton\n",
    "            x = df_boite[df_boite.canton==cant][year].values\n",
    "            # Get the percentage of automatic vehicles\n",
    "            percent = x[0]/sum(x)\n",
    "            # Build a table\n",
    "            df_boite_canton.loc[cant,year] = percent\n",
    "            \n",
    "# Restrain the set for the plot since we only have information on the victims from 1992\n",
    "df_boite_canton_toplot = df_boite_canton.drop(columns=['1990', '1991']).\\\n",
    "        reindex(df_boite_canton.index)\n",
    "\n",
    "# Plot with our function\n",
    "plot_high_correlation(x = df_boite_canton_toplot, y = df_victimes_cantons_normalized,\\\n",
    "                title='Correlation wrt. the percentage of automatic gear boxes',\\\n",
    "                xlabel='Percentage', ylabel='Number of victims',\\\n",
    "                labels=df_victimes_cantons_normalized.index,\\\n",
    "                name_save='correlation_gear_box_best', alpha=alpha,\\\n",
    "                threshold_of_interest=threshold_of_interest)"
   ]
